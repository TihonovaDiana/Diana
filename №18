#include <iostream>
using namespace std;
int main()
{
	int a, b, c, m;
	for (int i = 100; i < 1000; i++)
	{
		m = i;
		a = m % 10;
		m = m / 10;
		b = m % 10;
		m = m / 10;
		c = m;
		if (a == 0 || b == 0 || c == 0)
			continue;
		if ( i%a == 0 && i%b == 0 && i%c == 0)
			cout << i << endl;
	}
	system("pause");
	return 0;
}
